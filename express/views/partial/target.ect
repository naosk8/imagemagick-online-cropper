            <div class="baseImagePanel" id="baseImageCtrl" ng-controller="baseImageCtrl" ng-show="isShow()">
                <tabset>
                    <tab ng-repeat="image in imageList" heading={{image.name}} active="image.active" ng-click="updateBaseImageIndex($index)">
                        <div class="imgWrapper" id="{{image.name}}">
                            <canvas width={{image.width}} height={{image.height}}></canvas>
                            <img ng-src={{image.url}} width={{image.width}} height={{image.height}} />
                        </div>
                    </tab>
                </tabset>
            </div>